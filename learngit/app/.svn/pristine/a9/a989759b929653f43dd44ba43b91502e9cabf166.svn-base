package com.shanlin.p2p.app.model.enums;

public enum CreditAssignmentStatus {
	
	/** 
     * 待审核
     */
    DSH("待审核"),

    /** 
     * 转让中
     */
    ZRZ("转让中"),

    /** 
     * 已结束
     */
    YJS("已结束");

    protected final String chineseName;

    private CreditAssignmentStatus(String chineseName){
        this.chineseName = chineseName;
    }
    /**
     * 获取中文名称.
     * 
     * @return {@link String}
     */
    public String getChineseName() {
        return chineseName;
    }
}
