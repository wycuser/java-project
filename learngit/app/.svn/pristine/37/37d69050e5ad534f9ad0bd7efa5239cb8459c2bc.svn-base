package com.shanlin.p2p.app.model.enums;

public enum AreaLevel {
	/**
	 * 省级
	 */
	SHENG("省级"),

	/**
	 * 市级
	 */
	SHI("市级"),

	/**
	 * 县级
	 */
	XIAN("县级");

	protected final String chineseName;

	private AreaLevel(String chineseName) {
		this.chineseName = chineseName;
	}

	/**
	 * 获取中文名称.
	 * 
	 * @return {@link String}
	 */
	public String getChineseName() {
		return chineseName;
	}
}
