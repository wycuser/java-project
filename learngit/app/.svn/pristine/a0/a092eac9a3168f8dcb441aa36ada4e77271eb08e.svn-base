package com.shanlin.p2p.app.model.enums;


public enum OrdersStatus {
	/** 
     * 待提交
     */
    DTJ("待提交"),

    /** 
     * 已提交
     */
    YTJ("已提交"),

    /** 
     * 待确认
     */
    DQR("待确认"),

    /** 
     * 成功
     */
    CG("成功"),

    /** 
     * 失败
     */
    SB("失败");

    protected final String chineseName;

    private OrdersStatus(String chineseName){
        this.chineseName = chineseName;
    }
    /**
     * 获取中文名称.
     * 
     * @return {@link String}
     */
    public String getChineseName() {
        return chineseName;
    }
}
