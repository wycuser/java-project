package com.shanlin.p2p.app.model.enums;

/**
 * 标的状态
 * @author zheng xin
 * @createDate 2015年2月3日
 */
public enum BidStatus {
	/** 
     * 申请中
     */
    SQZ("申请中",""),

    /** 
     * 待审核
     */
    DSH("待审核",""),

    /** 
     * 待发布
     */
    DFB("待发布",""),

    /** 
     * 预发布
     */
    YFB("预发布","预"),

    /** 
     * 投标中
     */
    TBZ("投标中",""),

    /** 
     * 待放款
     */
    DFK("待放款","满"),

    /** 
     * 还款中
     */
    HKZ("还款中","还"),

    /** 
     * 已结清
     */
    YJQ("已结清","结"),

    /** 
     * 已流标
     */
    YLB("已流标","流"),

    /** 
     * 已作废
     */
    YZF("已作废",""),
    /** 
     * 已作废
     */
    ZF("已作废",""),

    /** 
     * 已垫付
     */
    YDF("已垫付","垫");

    protected final String chineseName;
    
    protected final String simpleName;

    private BidStatus(String chineseName, String simpleName){
        this.chineseName = chineseName;
        this.simpleName = simpleName;
    }
    /**
     * 获取中文名称.
     * 
     * @return {@link String}
     */
    public String getChineseName() {
        return chineseName;
    }
    
    public String getSimpleName() {
        return simpleName;
    }
}
